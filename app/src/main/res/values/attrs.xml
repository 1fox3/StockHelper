<resources>
    <declare-styleable name="SortTextView">
        <attr name="text" format="string"></attr>
        <attr name="textSize" format="float"></attr>
        <attr name="sortColumn" format="string"></attr>
        <attr name="sortType" format="enum">
            <enum name="no" value="-1" />
            <enum name="asc" value="0" />
            <enum name="desc" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="StockValueTextView">
        <attr name="value" format="string"></attr>
        <attr name="type" format="enum">
            <enum name="number" value="0" />
            <enum name="rate" value="1" />
        </attr>
        <attr name="unit" format="enum">
            <enum name="number" value="0" />
            <enum name="hand" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="StockIndexBlockView">
        <attr name="name" format="string"></attr>
        <attr name="currentPrice" format="float"></attr>
        <attr name="yesterdayClosePrice" format="float" />
    </declare-styleable>
    <declare-styleable name="StockRankInfoView">
        <attr name="stockName" format="string"></attr>
        <attr name="stockCode" format="string"></attr>
        <attr name="stockPrice" format="float" />
        <attr name="uptickRate" format="float" />
        <attr name="surgeRate" format="float" />
        <attr name="dealNum" format="float" />
        <attr name="dealMoney" format="float" />
    </declare-styleable>
</resources>
